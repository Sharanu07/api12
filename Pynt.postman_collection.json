		{
			"name": "Security Misconfiguration (OWASP-10 API7)",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"results = pm.response.json();",
							"if (pm.response.code == 202) {",
							"\tpm.test(\"\", () => {throw new Error(results)});",
							"\treturn",
							"}",
							"if(Object.keys(results).length > 0 ){",
